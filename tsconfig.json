{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDirs": ["lib", "test"],
    "strict": true,
    "strictPropertyInitialization": false,
    "declaration": true,
    "baseUrl": "./lib",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["lib"],
  "exclude": ["node_modules"]
}
